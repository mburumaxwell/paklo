# frozen_string_literal: true

source "https://rubygems.org"

# This Gemfile is used to track changes in dependabot-core which produces
# the docker images we use in this repo. This helps react to changes appropriately.
# A lock file is missing intentionally.
# There is no lockfile because we are only interested in tracking gem version changes.

gem "dependabot-omnibus", "~> 0.359.0"
